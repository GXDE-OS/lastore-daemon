#: ../src/lastore-daemon/inhibitor.go:30
msgid "Deepin Store"
msgstr "Deepin Winkel"

#: ../src/lastore-daemon/inhibitor.go:31
msgid "System is updating, please shut down or reboot later."
msgstr "Het systeem wordt bijgewerkt; sluit hierna af of herstart."

#: ../src/lastore-session-helper/check_source.go:47
msgid "Restore"
msgstr "Herstellen"

#: ../src/lastore-session-helper/check_source.go:58
#: ../src/lastore-session-helper/job.go:325
msgid "Cancel"
msgstr "Annuleren"

#: ../src/lastore-session-helper/job.go:294
msgid "Reboot Now"
msgstr "Nu herstarten"

#: ../src/lastore-session-helper/job.go:308
msgid "Reboot Later"
msgstr "Later herstarten"

#: ../src/lastore-session-helper/job.go:317
msgid "Retry"
msgstr "Opnieuw proberen"

#: ../src/lastore-session-helper/notify.go:70
msgid "%q installed successfully."
msgstr "%q is succesvol geïnstalleerd."

#: ../src/lastore-session-helper/notify.go:73
msgid "%q failed to install."
msgstr "Het installeren van %q is mislukt."

#: ../src/lastore-session-helper/notify.go:82
msgid "%q removed successfully."
msgstr "%q is succesvol verwijderd."

#: ../src/lastore-session-helper/notify.go:84
msgid "%q failed to remove."
msgstr "Het verwijderen van %q is mislukt."

#: ../src/lastore-session-helper/notify.go:91
msgid "%q failed to download."
msgstr "Het downloaden van %q is mislukt."

#: ../src/lastore-session-helper/notify.go:97
msgid ""
"In order to prevent automatic shutdown, please plug in for normal update."
msgstr "Om te voorkomen dat de computer automatisch wordt afgesloten, moet u deze op de netstroom aansluiten."

#: ../src/lastore-session-helper/notify.go:105
msgid "Some functions will take effect after rebooting."
msgstr "Sommige functies zullen worden geactiveerd na een herstart."

#: ../src/lastore-session-helper/notify.go:105
#: ../src/lastore-session-helper/notify.go:107
msgid "Updated successfully!"
msgstr "Het bijwerken is succesvol voltooid!"

#: ../src/lastore-session-helper/notify.go:111
msgid "Failed to update."
msgstr "Het bijwerken is mislukt."

#: ../src/lastore-session-helper/notify.go:121
#, c-format
msgid "%d software need to be updated."
msgstr "%d software-items moet(en) worden bijgewerkt."

#: ../src/lastore-session-helper/notify.go:123
msgid "Some patches need to be updated."
msgstr "Enkele patches moeten worden bijgewerkt."

#: ../src/lastore-session-helper/notify.go:125
#, c-format
msgid "Some patches and %d software need to be updated."
msgstr "Enkele patches en %d software-items moeten worden bijgewerkt."

#: ../src/lastore-session-helper/notify.go:133
msgid "Download"
msgstr "Downloaden"

#: ../src/lastore-session-helper/notify.go:141
msgid "Failed to download upgradeable packages."
msgstr "Het downloaden van op te waarderen pakketten is mislukt."

#: ../src/lastore-session-helper/notify.go:146
msgid "Package cache wiped"
msgstr "Pakketcache opgeschoond"

#: ../src/lastore-session-helper/notify.go:151
msgid ""
"Your system source has been modified, please restore to official source for "
"your normal use"
msgstr "Je systeembron is gewijzigd. Herstel de officiële bron voor normaal gebruik."
